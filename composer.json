{
    "name": "hackzilla/ticket-bundle",
    "description": "This Bundle provides ticketing functionality for Symfony2 applications. Multilingual capable",
    "keywords": ["ticket", "ticketing", "help desk", "helpdesk", "support", "multilingual", "english"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Platt",
            "email": "github@ofdan.co.uk",
            "homepage": "http://www.hackzilla.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php":  ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "hackzilla/interface":  "dev-master",
        "knplabs/knp-paginator-bundle":  "dev-master"
    },
    "suggest": {
        "twitter/bootstrap": "used to define the layout (v3)"
    },
    "autoload": {
        "psr-0": {
            "Hackzilla\\Bundle\\TicketBundle": ""
        }
    },
    "target-dir": "Hackzilla/Bundle/TicketBundle"
}
